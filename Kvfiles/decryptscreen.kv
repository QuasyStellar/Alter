<Decrypt>:
	name: 'decrypt'
    Image:
        source: 'Assets/decrypt/decryptscreen.png'
        allow_stretch: True
        keep_ratio: False
    MDTextButton:
        size_hint: .2, .15
        halign: 'center'
        pos_hint: {'center_x': .89, 'center_y': .08}
        font_size: dp(25)
        on_release:
            root.manager.current =  'mosloged'
            root.ids.scrollid.clear_widgets()
        Image:
            source: 'Assets/exitbutton.png'
            center_x: self.parent.center_x
            center_y: self.parent.center_y
            allow_stretch: True
            size: self.parent.size
    ScrollView:
        size_hint: .5, .7
        md_bg_color: 1,1,1,0
        pos_hint: {'center_x': .5, 'center_y': .37}
        GridLayout:
            scroll_distance: 30
            cols:1
            spacing:10 
            size_hint_y: None
            height: self.minimum_height
            id: scrollid