<Loading>:
    name: "load"
    canvas.before:
        Color:
            rgba: 176/255,0,0, 1
        Rectangle:
            pos: self.pos
            size: self.size
    MDSpinner:
        size_hint: None, None
        size: dp(60), dp(60)
        color: 1,1,1,1
        pos_hint: {'center_x': .5, 'center_y': .5}
        active: True 
<Loadingoms>:
    name: "loadoms"
