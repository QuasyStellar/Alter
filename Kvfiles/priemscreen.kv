<Priem>:
	name: 'priem'
    Image:
        source: 'Assets/priemscreen/priemscreen.png'
        allow_stretch: True
        keep_ratio: False
	MDTextButton:
        orientation: "vertical"
        pos_hint: {'center_x': .583, 'center_y': .174}
        size_hint: .21, .125
        radius: [30]
        on_release: root.exits()
        Image:
            source: 'Assets/priemscreen/exitbut.png'
            center_x: self.parent.center_x
            center_y: self.parent.center_y
            size: self.parent.size
            keep_ratio: False
            allow_stretch: True
    MDTextButton:
        size_hint: .271, .125
        halign: 'center'
        pos_hint: {'center_x': .838, 'center_y': .174}
        font_size: dp(25)
        on_release: root.helzy()
        Image:
            source: 'Assets/priemscreen/startbut.png'
            center_x: self.parent.center_x
            center_y: self.parent.center_y
            size: self.parent.size
            keep_ratio: False
            allow_stretch: True