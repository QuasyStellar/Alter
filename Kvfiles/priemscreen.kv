<Priem>:
	name: 'priem'
    Image:
        source: 'Assets/priemscreen/priemscreen.png'
        allow_stretch: True
        keep_ratio: False
	MDTextButton:
        pos_hint: {'center_x': .583, 'center_y': .174}
        size_hint: .21, .13
        on_release: root.exits()
        Image:
            source: 'Assets/priemscreen/exitbut.png'
            center_x: self.parent.center_x
            center_y: self.parent.center_y
            size: self.parent.size
            keep_ratio: False
            allow_stretch: True
    MDTextButton:
        size_hint: .271, .13
        pos_hint: {'center_x': .838, 'center_y': .174}
        on_release: root.helzy()
        Image:
            source: 'Assets/priemscreen/startbut.png'
            center_x: self.parent.center_x
            center_y: self.parent.center_y
            size: self.parent.size
            keep_ratio: False
            allow_stretch: True
    MDLabel:
        id: time
        bold: True
        theme_text_color: 'Custom'
        text_color: 'white'
        font_name: 'Assets/fonts/roboto.ttf'
        markup: True
        pos_hint: {'center_x': .08, 'center_y': .93}
        font_size: dp(80)
        halign: 'center'
    MDLabel:
        id: week
        bold: True
        theme_text_color: 'Custom'
        text_color: 'white'
        markup: True
        font_name: 'Assets/fonts/roboto.ttf'
        pos_hint: {'center_x': .081, 'center_y': .87}
        font_size: dp(38)
        halign: 'center'