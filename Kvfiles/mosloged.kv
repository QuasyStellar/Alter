<MOSLoged>:
    name: 'mosloged'
    Image:
        source: 'Assets/mosloged/mainmenubgmos.png'
        allow_stretch: True
        keep_ratio: False
    MDTextButton:
        size_hint: .2, .15
        halign: 'center'
        pos_hint: {'center_x': .88, 'center_y': .08}
        font_size: dp(25)
        on_release: root.exits()
        Image:
            source: 'Assets/mosloged/exitbut.png'
            center_x: self.parent.center_x
            center_y: self.parent.center_y
            allow_stretch: True
            size: self.parent.size
    MDLabel:
        id: authname
        text:'Если вы не видете здесь свою фамилию вам очень повезло'
        text_color: get_color_from_hex('#72C3AC')
        theme_text_color: 'Custom'
        halign: 'center'
        size_hint: .27, .06
        pos_hint: {'center_x': .5, 'center_y': .09}
        font_size: dp(40) 
        font_name: 'Assets/roboto'

    MDTextButton:
        orientation: "vertical"
        pos_hint: {'center_x': .367, 'center_y': .575}
        size_hint: .106, .069
        on_release: root.manager.get_screen('loged').newzapis()
        Image:
            source: 'Assets/mosloged/myzapis.png'
            center_x: self.parent.center_x
            center_y: self.parent.center_y
            size: self.parent.size
    MDTextButton:
        orientation: "vertical"
        pos_hint: {'center_x': .488, 'center_y': .575}
        size_hint: .1226, .069
        on_release: root.manager.get_screen('loged').prosmotrnapr()
        Image:
            source: 'Assets/mosloged/mynapr.png'
            center_x: self.parent.center_x
            center_y: self.parent.center_y
            size: self.parent.size
    MDTextButton:
        orientation: "vertical"
        pos_hint: {'center_x': .625, 'center_y': .575}
        size_hint: .13125, .069
        on_release: root.manager.get_screen('loged').prosmotrnapr()
        Image:
            source: 'Assets/mosloged/myprik.png'
            center_x: self.parent.center_x
            center_y: self.parent.center_y
            size: self.parent.size
    MDTextButton:
        orientation: "vertical"
        pos_hint: {'center_x': .5, 'center_y': .7059}
        size_hint: .3906, .1159
        on_release: root.manager.get_screen('loged').zapisi()
        Image:
            source: 'Assets/mosloged/emiasbutton.png'
            center_x: self.parent.center_x
            center_y: self.parent.center_y
            size: self.parent.size

    MDTextButton:
        orientation: "vertical"
        pos_hint: {'center_x': .405, 'center_y': .4267}
        size_hint: .1886, .152
        on_release: root.manager.current = "lkcard"
        Image:
            source: 'Assets/mosloged/lkcardbutton.png'
            center_x: self.parent.center_x
            center_y: self.parent.center_y
            size: self.parent.size
    MDTextButton:
        orientation: "vertical"
        pos_hint: {'center_x': .5, 'center_y': .2454}
        size_hint: .3906, .1326
        on_release: root.manager.current = 'priem'
        Image:
            source: 'Assets/mosloged/priemmosbutton.png'
            center_x: self.parent.center_x
            center_y: self.parent.center_y
            size: self.parent.size
    MDTextButton:
        orientation: "vertical"
        pos_hint: {'center_x': .60, 'center_y': .4267}
        size_hint: .1886, .152
        on_release: root.manager.get_screen('decrypt').myanaliz()
        Image:
            source: 'Assets/mosloged/cardanalizbutton.png'
            center_x: self.parent.center_x
            center_y: self.parent.center_y
            size: self.parent.size
    MDLabel:
        id: time
        bold: True
        theme_text_color: 'Custom'
        text_color: 'white'
        markup: True
        pos_hint: {'center_x': .08, 'center_y': .93}
        font_size: dp(100)
        font_name: 'Assets/roboto'
        halign: 'center'
    MDLabel:
        id: week
        bold: True
        theme_text_color: 'Custom'
        text_color: 'white'
        markup: True
        pos_hint: {'center_x': .08, 'center_y': .87}
        font_size: dp(50)
        font_name: 'Assets/roboto'
        halign: 'center'